{
  "name": "mltshp-patterns",
  "version": "1.0.0",
  "author": "Scott Vandehey",
  "description": "Pattern Library for MLTSHP Apps",
  "homepage": "https://github.com/MLTSHP/mltshp-patterns#readme",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MLTSHP/mltshp-patterns.git"
  },
  "bugs": {
    "url": "https://github.com/MLTSHP/mltshp-patterns/issues"
  },
  "main": "./dist/mltshp-patterns.js",
  "unpkg": "./dist/mltshp-patterns.min.js",
  "module": "./dist/mltshp-patterns.mjs",
  "exports": "./dist/mltshp-patterns.mjs",
  "files": [
    "/dist",
    "/src/**/*.scss",
    "/src/assets",
    "!.gitignore"
  ],
  "scripts": {
    "storybook": "yarn preprocess && run-p watch-preprocess start-storybook",
    "start-storybook": "start-storybook -p 6006 -s static",
    "build-storybook": "yarn preprocess && build-storybook -s static",
    "preprocess": "gulp theo-to-mdx",
    "watch-preprocess": "gulp watch-preprocess",
    "build": "gulp build",
    "test": "yarn lint",
    "check-lint": "run-s check-lint:*",
    "check-lint:css": "stylelint '**/*.scss'",
    "check-lint:js": "eslint . --ext .js,.mdx",
    "lint": "run-s lint:*",
    "lint:css": "stylelint --fix '**/*.scss'",
    "lint:js": "eslint . --fix --ext .js,.mdx",
    "prepublishOnly": "yarn && yarn check-lint && yarn preprocess && yarn build"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@cloudfour/eslint-plugin": "7.0.0",
    "@rollup/plugin-multi-entry": "3.0.0",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@storybook/addon-a11y": "5.3.14",
    "@storybook/addon-backgrounds": "5.3.14",
    "@storybook/addon-docs": "5.3.14",
    "@storybook/addon-knobs": "5.3.14",
    "@storybook/addon-viewport": "5.3.14",
    "@storybook/html": "5.3.14",
    "@storybook/preset-scss": "1.0.2",
    "@svgr/webpack": "5.3.0",
    "babel-loader": "8.1.0",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-mdx": "1.6.8",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "gulp": "4.0.2",
    "gulp-postcss": "8.0.0",
    "gulp-prettier": "3.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.0.2",
    "gulp-theo": "2.0.1",
    "js-yaml": "3.13.1",
    "lodash": "4.17.15",
    "npm-run-all": "4.1.5",
    "plugin-error": "1.0.1",
    "prettier": "1.19.1",
    "remark-preset-lint-recommended": "3.0.4",
    "remark-preset-prettier": "0.4.0",
    "require-dir": "1.2.0",
    "rollup": "2.1.0",
    "rollup-plugin-terser": "5.3.0",
    "sass": "1.26.2",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "13.2.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-spaceninja": "7.0.0",
    "stylelint-prettier": "1.1.2",
    "theo": "8.1.5",
    "tiny-glob": "0.2.6"
  }
}
